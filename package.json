{
  "private": true,
  "name": "purescript-grain-virtualized",
  "scripts": {
    "spago": "spago",
    "watch": "spago build -w -u '--strict --stash --censor-lib --is-lib=.spago'",
    "clean": "rm -rf output",
    "start": "spago -x example.dhall build -u '--strict --stash --censor-lib --is-lib=.spago' && parcel ./example/index.html",
    "demo": "rm ./index.*.js && spago -x example.dhall build -u '--strict --stash --censor-lib --is-lib=.spago' && parcel build --no-source-maps --dist-dir ./ --public-url ./ ./example/index.html",
    "pulp:publish": "pulp publish",
    "pulp:login": "pulp login"
  },
  "dependencies": {
    "bower": "^1.8.14",
    "parcel": "^2.8.0",
    "pulp": "^16.0.2",
    "purescript": "^0.15.6",
    "purescript-psa": "^0.8.2",
    "spago": "^0.20.9"
  }
}
